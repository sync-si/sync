{
    "name": "sync3-web",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "story:dev": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview",
        "lint": "oxlint . --fix && eslint . --fix --cache",
        "format": "prettier --write --experimental-cli src/",
        "check:type": "vue-tsc --noEmit",
        "check:lint": "oxlint . && eslint . --cache",
        "check:format": "prettier --check --experimental-cli src/",
        "check:all": "bun run check:type && bun run check:lint && bun run check:format"
    },
    "dependencies": {
        "@floating-ui/vue": "^1.1.9",
        "@fontsource-variable/inter": "^5.2.8",
        "@regle/core": "^1.16.1",
        "@regle/rules": "^1.16.1",
        "@sync/wire": "workspace:*",
        "@vueuse/core": "^14.1.0",
        "crypto-js": "^4.2.0",
        "pinia": "^3.0.4",
        "slugify": "^1.6.6",
        "vue": "^3.5.26",
        "vue-router": "4"
    },
    "devDependencies": {
        "@histoire/plugin-vue": "^1.0.0-alpha.5",
        "@spiriit/vite-plugin-svg-spritemap": "^6.0.0",
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^24.10.1",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.39.2",
        "eslint-plugin-oxlint": "~1.35.0",
        "eslint-plugin-vue": "~10.6.2",
        "histoire": "^1.0.0-alpha.5",
        "oxlint": "^1.36.0",
        "prettier": "^3.7.4",
        "typescript": "~5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vue-tsc": "^3.2.1"
    }
}
